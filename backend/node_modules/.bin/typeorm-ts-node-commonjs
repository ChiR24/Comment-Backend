#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_re_9dbd8564e3525f114ba99fd2baccc261/node_modules/typeorm/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_re_9dbd8564e3525f114ba99fd2baccc261/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_re_9dbd8564e3525f114ba99fd2baccc261/node_modules/typeorm/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_re_9dbd8564e3525f114ba99fd2baccc261/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
fi
