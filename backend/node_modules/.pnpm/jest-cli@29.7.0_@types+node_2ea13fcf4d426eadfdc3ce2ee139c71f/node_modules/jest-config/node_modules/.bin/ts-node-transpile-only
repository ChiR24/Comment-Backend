#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._2ea25087c3582887f9efd8ff40216170/node_modules/ts-node/dist/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._2ea25087c3582887f9efd8ff40216170/node_modules/ts-node/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._2ea25087c3582887f9efd8ff40216170/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._2ea25087c3582887f9efd8ff40216170/node_modules/ts-node/dist/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._2ea25087c3582887f9efd8ff40216170/node_modules/ts-node/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1._2ea25087c3582887f9efd8ff40216170/node_modules:/mnt/x/OA Assignment/Sanctity/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.2_@swc+core@1._2ea25087c3582887f9efd8ff40216170/node_modules/ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.2_@swc+core@1._2ea25087c3582887f9efd8ff40216170/node_modules/ts-node/dist/bin-transpile.js" "$@"
fi
